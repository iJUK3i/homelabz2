#include <unistd.h>

void ft_print_alphabet(void);

int ft_putchar(char x)
{
	write (1, &x, 1);
	return (0);
}

int ft_print_alpha()
{
	char x;
	x = 'a';
	while (x <= 'z')
	{
		ft_putchar(x);
		x++;
	}
}

int main()
{
	ft_putchar('\n');
	ft_print_alpha();
	ft_putchar('\n');
	ft_putchar('\n');
	return (0);
}
